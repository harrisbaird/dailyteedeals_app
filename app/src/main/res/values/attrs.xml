<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseTheme">
        <attr name="actionBarIconColor" format="color" />
        <attr name="actionBarInsetStart" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DrawShadowFrameLayout">
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowVisible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForeground" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="FabView">

        <attr name="fab_attachTo" format="reference" />
        <attr name="fab_attachAt" format="enum">
            <enum name="top_left" value="1" />
            <enum name="top_right" value="2" />
            <enum name="bottom_left" value="3" />
            <enum name="bottom_right" value="4" />
        </attr>
        <attr name="fab_attachType" format="enum">
            <enum name="border" value="1" />
            <enum name="inside" value="2" />
        </attr>
        <attr name="fab_size" format="enum">
            <enum name="normal" value="1" />
            <enum name="small" value="2" />
        </attr>
        <attr name="fab_padding" format="dimension" />
        <attr name="fab_revealAfterMs" format="integer" />

    </declare-styleable>
</resources>